{
  "name": "bui-css",
  "version": "2.2.0",
  "description": "A contemporary, approachable css library for prototyping and beyond",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "test": "gulp test",
    "dev": "gulp css:watch",
    "build": "gulp css:min",
    "release": "npm run build && standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ndchristie/bui.git"
  },
  "keywords": [
    "lightweight",
    "contemporary",
    "ui",
    "library",
    "css",
    "scss",
    "sass",
    "theme"
  ],
  "author": "N.D.Christie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ndchristie/bui/issues"
  },
  "homepage": "https://github.com/ndchristie/bui#readme",
  "devDependencies": {
    "cssnano": "^3.10.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-postcss": "^6.4.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.4.1",
    "postcss-convert-values": "^2.6.1",
    "postcss-cssnext": "^2.10.0",
    "postcss-discard-comments": "^2.0.4",
    "postcss-discard-duplicates": "^2.1.0",
    "postcss-discard-empty": "^2.1.0",
    "postcss-discard-unused": "^2.2.3",
    "postcss-merge-idents": "^2.1.7",
    "postcss-merge-longhand": "^3.0.0",
    "postcss-merge-selectors": "0.0.6",
    "postcss-minify-font-values": "^1.0.5",
    "postcss-minify-selectors": "^2.1.1",
    "postcss-normalize-charset": "^1.1.1",
    "standard-version": "^4.3.0",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0"
  },
  "files": [
    "lib/"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
